@model IEnumerable<IEXTrading.Models.Company>
<h2>Symbols</h2>
<hr />
<table class="table">
    <thead>
        <tr>
            <th>
                Symbol
            </th>
            <th>
                Name
            </th>
            <th>
                Action
            </th>
 
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.symbol)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.name)
                </td>
                <td>
                    <a asp-controller="Home" asp-action="SaveFinacialReportsDB" asp-route-symbol=@item.symbol>Copy Financials to Application</a>
                    
                </td>

            </tr>
        }
        
    </tbody>
</table>
<script>
    alertDbSave(@ViewBag.dbSuccessComp);
</script>
